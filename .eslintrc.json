{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "no-var": "error",
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "no-multiple-empty-lines": "off",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
